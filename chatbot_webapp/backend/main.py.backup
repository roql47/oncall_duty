                                # ì „ë‚  ìŠ¤ì¼€ì¤„ì´ë©´ì„œ ì•¼ê°„ ê·¼ë¬´ì¸ ê²½ìš°
                                if schedule_datetime.date() < query_date.date() and end_total_minutes <= start_total_minutes:
                                    overnight_schedules.append(schedule)
